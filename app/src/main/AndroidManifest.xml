<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ananda.oleholehbali">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Tokolaptop">
        <activity
            android:name="com.ananda.oleholehbali.AkunActivity"
            android:exported="false" />
        <activity
            android:name="com.ananda.oleholehbali.KeranjangActivity"
            android:exported="false" />
        <activity
            android:name="com.ananda.oleholehbali.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ananda.oleholehbali.DashboardActivity"
            android:exported="false" />
        <activity
            android:name="com.ananda.oleholehbali.RegisterActivity"
            android:exported="false" />
        <activity
            android:name="com.ananda.oleholehbali.LoginActivity"
            android:exported="false"/>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>